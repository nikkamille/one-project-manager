<h2><%= @project.title %></h2>

<ul>
    <% @project.tasks.each do |task| %>
        <li><%= link_to task.name, task_path(task) %></li>
    <% end %>
</ul>


<%= button_to "Edit Project", edit_project_path(@project), method: :get %>
<%= button_to "Delete Project", project_path(@project), method: :delete %>